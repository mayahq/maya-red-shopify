<%
    const prefix = isConfig ? 'node-config-input' : 'node-input'
-%>
<div class="form-row">
    <label for="<%= prefix %>-<%= property.name %>"><%= property.displayName %></label>
    <input type="<%- property.password ? 'password' : 'text' %>" id="<%= prefix %>-<%= property.name %>"></input>
    <input type="hidden" id="<%= prefix %>-payloadType<%= property.name %>">
</div>