<% 
    const typeField = `payloadType${property.name}`
-%> 
if (this.<%= typeField %> === null) {
    if (this.<%= property.name %> === "") {
        this.<%= typeField %> = "date" 
    } else {
        this.<%= typeField %> = "num" 
    }
} else if (this.<%= typeField %> === 'string' || this.<%= typeField %> === 'none') {
    this.<%= typeField %> = "num" 
}

$("#node-input-<%= typeField %>").val(this.<%= typeField %>)
$("#node-input-<%= property.name %>").typedInput({
    default: "num",
    typeField: $("#node-input-<%= typeField %>"),
    types: ["flow", "global", "msg", "num"]
})
$("#node-input-<%= property.name %>").typedInput("type", this.<%= typeField %>)